    "new_raster_path = r'E:\\wenqu\\environmental_factors\\environment_update1\\soil\\soil_ph_5n.tif'\n",
    "\n",
    "# 打开新的栅格文件\n",
    "with rasterio.open(new_raster_path) as src:\n",
    "    # 获取栅格数据的仿射变换参数\n",
    "    transform = src.transform\n",
    "    \n",
    "    for index, row in filtered_polygons.iterrows():\n",
    "        geometry = row['geometry']\n",
    "        # 使用栅格进行遮罩\n",
    "        out_image, out_transform = rasterio.mask.mask(src, [geometry], crop=True, all_touched=True, nodata=src.nodata)\n",
    "        \n",
    "        # 计算非nodata像素的数量\n",
    "        new_pixel_count = (out_image != src.nodata).sum()\n",
    "        filtered_polygons.at[index, 'soil_ph_count'] = new_pixel_count\n",
    "        \n",
    "        if new_pixel_count > 0:\n",
    "            # 将nodata值转换为NaN，进行计算\n",
    "            out_image = out_image.astype(float)\n",
    "            out_image[out_image == src.nodata] = np.nan\n",
    "            new_mean_val = np.nanmean(out_image)\n",
    "            filtered_polygons.at[index, 'soil_ph_value'] = new_mean_val\n",
    "        else:\n",
    "            # 获取多边形的中心点\n",
    "            centroid = geometry.centroid\n",
    "            x, y = centroid.x, centroid.y\n",
    "            # 将中心点的地理坐标转换为栅格坐标\n",
    "            row, col = rasterio.transform.rowcol(transform, x, y)\n",
    "            # 尝试获取中心点下的栅格值\n",
    "            if (0 <= row < src.height) and (0 <= col < src.width):  # 确保坐标在栅格范围内\n",
